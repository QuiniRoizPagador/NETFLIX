<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data noupdate="1">
		<!-- Users -->
		<record id="user1" model="upoflix.user">
			<field name="name">Manuela Antonieta</field>
			<field name="surname">Pepequénico Calisténico</field>
			<field name="mail">amapeca@gmail.com</field>
			<field name="password">papeleta1</field>
		</record>

		<record id="user2" model="upoflix.user">
			<field name="name">Pepe Manuel</field>
			<field name="surname">Del Serranito Ibérico</field>
			<field name="mail">pepemase@gmail.com</field>
			<field name="password">jamonypimiento</field>
		</record>

		<!-- Genders -->
		<record id="gender1" model="gender">
			<field name="name">Drama Adolescente</field>
			<field name="description">El cine adolescente (también conocido como teen) es
				un género cinematográfico destinado a adolescentes y adultos
				jóvenes, cuyo argumento está frecuentemente basado a los intereses
				especiales de los más jóvenes, como la mayoría de edad, el primer
				amor, la rebeldía, conflictos con los padres, angustia adolescente,
				y la alienación. A menudo, estos temas serios, son presentados de
				una manera superficial, estereotipada o trivializados.
			</field>
		</record>

		<record id="gender2" model="gender">
			<field name="name">Fantástico</field>
			<field name="description">
				El género fantástico, es un género artístico en el
				que hay presencia de elementos que rompen con la realidad
				establecida.
			</field>
		</record>

		<!-- OSDs -->
		<record id="osd1" model="osd">
			<field name="name">Por 13 razones BSO</field>
			<field name="composer">Eskmo</field>
			<field name="producer">Joseph Incaprera</field>
			<field name="studio">Warm studios</field>
		</record>

		<record id="osd2" model="osd">
			<field name="name">(Des)encanto BSO</field>
			<field name="composer">Mark Mothersbaugh</field>
			<field name="producer">Mark Mothersbaugh</field>
		</record>

		<record id="osd3" model="osd">
			<field name="name">El Indomable Will Hunting BSO</field>
			<field name="composer">Danny Elfman</field>
			<field name="producer">Danny Elfman</field>
		</record>

		<record id="osd4" model="osd">
			<field name="name">Harry Potter BSO</field>
			<field name="composer">Charles Edward</field>
			<field name="producer">Charles Edward</field>
		</record>

		<!-- Partakers -->
		<record id="partaker1" model="partaker">
			<field name="name">Brian Yorkey</field>
			<field name="country" ref="base.uk" />
		</record>

		<record id="partaker2" model="partaker">
			<field name="name">Matt Groening</field>
			<field name="country" ref="base.us" />
		</record>

		<!-- Series -->
		<record id="serie1" model="serie">
			<field name="name">Por Trece Razones</field>
			<field name="year">2017-03-31</field>
			<field name="total_score">7</field>
			<field name="director" ref="partaker1" />
			<field name="genders" eval="[(6, 0, [gender1, gender2])]" />
			<field name="actors" eval="[(6, 0, [partaker1, partaker2])]" />
			<field name="osd" ref="osd1" />
		</record>

		<record id="serie2" model="serie">
			<field name="name">(Des)encanto</field>
			<field name="year">2018-08-17</field>
			<field name="director" ref="partaker2" />
			<field name="serie_fav" eval="[(6, 0, [user1])]" />
			<field name="genders" eval="[(6, 0, [gender1, gender2])]" />
			<field name="actors" eval="[(6, 0, [partaker1])]" />
			<field name="osd" ref="osd2" />
		</record>

		<!-- Seasons -->

		<record id="season1" model="season">
			<field name="season_number">1</field>
			<field name="serie_id" ref="serie1" />
		</record>

		<record id="season2" model="season">
			<field name="season_number">1</field>
			<field name="serie_id" ref="serie2" />
		</record>

		<!-- Chapters -->

		<record id="chapter1" model="chapter">
			<field name="name">Tape 1</field>
			<field name="emission_date">2017-03-31</field>
			<field name="season_id" ref="season1" />
		</record>

		<record id="chapter2" model="chapter">
			<field name="name">A Princess, an Elf and a Demon Walk into a Bar
			</field>
			<field name="emission_date">2018-08-17</field>
			<field name="season_id" ref="season2" />
		</record>

		<!-- Peliculas -->
		<record id="film1" model="film">
			<field name="name">El indomable Will Hunting</field>
			<field name="year">1997-12-01</field>
			<field name="director" ref="partaker1" />
			<field name="genders" eval="[(6, 0, [gender1, gender2])]" />
			<field name="osd" ref="osd3" />
			<field name="premiere_date">1997-12-01</field>
		</record>

		<record id="film2" model="film">
			<field name="name">Harry Potter</field>
			<field name="year">2007-07-21</field>
			<field name="director" ref="partaker2" />
			<field name="genders" eval="[(6, 0, [gender1])]" />
			<field name="osd" ref="osd4" />
			<field name="premiere_date">2007-07-21</field>
		</record>


		<!-- Scores -->
		<record id="score1" model="score">
			<field name="score">9</field>
			<field name="users_score" ref="user1" />
			<field name="serie_id" ref="serie1" />
		</record>

		<record id="score2" model="score">
			<field name="score">5</field>
			<field name="users_score" ref="user2" />
			<field name="serie_id" ref="serie1" />
		</record>


	</data>
</openerp>
